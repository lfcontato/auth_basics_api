{
    "version": 2,
    "builds": [
        {
            "src": "auth_app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "auth_app/main.py"
        }
    ],
    "installCommand": "pip install poetry && poetry install --no-root"
}